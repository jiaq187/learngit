    <dubbo:service interface="com.ddsc.api.pay.task.TradeTaskService" ref="tradeTaskService" protocol="hessian" retries="0"  version="1.0.0">
    </dubbo:service>
    <dubbo:service interface="com.ddsc.api.pay.task.KuaiqianAutoPayTaskService" ref="kuaiqianAutoPayTaskService" protocol="hessian" retries="0"  version="1.0.0">
	</dubbo:service>
	<dubbo:service interface="com.ddsc.api.pay.pc.TradeService" ref="pcTradeService" protocol="hessian" retries="0"  version="1.0.0">
    </dubbo:service>
</beans>