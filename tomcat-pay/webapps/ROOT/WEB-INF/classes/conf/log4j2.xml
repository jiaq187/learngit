<?xml version="1.0" encoding="UTF-8"?>  
<configuration status="OFF" monitorInterval="3600">  
	<properties>
		<property name="LOG_HOME">/home/ddsc/local/var/ddsc_log</property>
		<property name="SERVER_NAME">ddsc_pay</property>
		<property name="BACKUP_HOME">backup</property>
		<property name="PERFORMANCE_NAME">performance</property>
		<property name="EXCEPTION_NAME">exception</property>
		<property name="SYSTEMLOG_NAME">systemlog</property>
		<property name="KUAIQIANLOG_NAME">kuaiqianlog</property>
		<property name="BAOFOO_NAME">baofoolog</property>
		<property name="SHENGFUTONGLOG_NAME">shengfutonglog</property>
		<property name="PAYSUCCESSLOG_NAME">paysuccesslog</property>
		<property name="ONCURRENTLOG_NAME">concurrentlog</property>
		<property name="TASK_LOG">tasklog</property>
		<property name="SQLLOG_NAME">sqlLog</property>
