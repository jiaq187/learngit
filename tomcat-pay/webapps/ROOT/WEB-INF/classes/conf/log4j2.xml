		</root>
		<logger name="org.springframework" level="info" additivity="false">
			<appender-ref ref="Console" />
		</logger>
		<logger name="org.apache.zookeeper" level="info" additivity="false">
			<appender-ref ref="Console" />
			<appender-ref ref="${SERVER_NAME}"/>
		</logger>
		<logger name="com.alibaba.dubbo" level="info" additivity="false">
			<appender-ref ref="Console" />
			<appender-ref ref="${SERVER_NAME}"/>
		</logger>
		<logger name="com.ddsc.model" level="debug" additivity="false">
			<appender-ref ref="Console" />
		</logger>
		<AsyncLogger name="performanceLog" level="info" additivity="false" includeLocation="true">
			<appender-ref ref="${PERFORMANCE_NAME}"/>
		</AsyncLogger>
		<AsyncLogger name="exceptionLog" level="info" additivity="false" includeLocation="true">
			<appender-ref ref="${EXCEPTION_NAME}"/>
		</AsyncLogger>
		<AsyncLogger name="systemLog" level="info" additivity="false" includeLocation="true">
