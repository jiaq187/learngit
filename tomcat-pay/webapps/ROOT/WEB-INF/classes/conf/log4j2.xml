	<appenders>  
		<Console name="Console" target="SYSTEM_OUT">  
			<PatternLayout pattern="%d{HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n"/>  
		</Console>
		<RollingFile name="${SERVER_NAME}" fileName="${LOG_HOME}/${SERVER_NAME}/${SERVER_NAME}.log"
			filePattern="${LOG_HOME}/${SERVER_NAME}/${BACKUP_HOME}/${SERVER_NAME}/${SERVER_NAME}_%d{yyyy-MM-dd}_%i.log.gz">
			<PatternLayout pattern="%d{HH:mm:ss.SSS z} [%t] %-5level %logger{36} - %msg%n"/>
			<Policies>
				<TimeBasedTriggeringPolicy modulate="true" interval="1"/>
				<SizeBasedTriggeringPolicy size="51200 KB"/>
			</Policies>
			<!-- <DefaultRolloverStrategy max="20"/> -->
